    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
